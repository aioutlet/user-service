# Development Environment Configuration for User Service
NODE_ENV=development
SERVICE_NAME=user-service
LOG_LEVEL=debug
LOG_FORMAT=console
LOG_TO_FILE=false
LOG_TO_CONSOLE=true
LOG_FILE_PATH=user-service.log

# Port configuration
PORT=5000

# Database configuration
MONGODB_URI=mongodb://localhost:27017/user-db

# JWT Configuration
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=1d

# CORS configuration
CORS_ORIGIN=http://localhost:3000
