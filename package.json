{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User management microservice for AIOutlet platform.",
  "main": "src/server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aioutlet/user-service.git"
  },
  "keywords": [
    "aioutlet",
    "microservice",
    "node",
    "express",
    "mongo"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aioutlet/user-service/issues"
  },
  "homepage": "https://github.com/aioutlet/user-service#readme",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dapr": "cross-env DAPR_ENABLED=true dotenv -e .env -- dapr run --app-id user-service --app-port 3002 --dapr-http-port 3502 --dapr-grpc-port 50003 --log-level warn --config ./.dapr/config.yaml --resources-path ./.dapr/components -- node src/server.js",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --coverage --runInBand",
    "test:integration": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/integration",
    "test:unit": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/unit",
    "test:e2e": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --runInBand tests/e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "node src/scripts/seed.js",
    "clear": "node src/scripts/clearUsers.js"
  },
  "dependencies": {
    "@dapr/dapr": "^3.6.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "babel-jest": "^30.0.0",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.34.0",
    "jest": "^30.0.5",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
